        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/red_ellipse" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="313dp"
